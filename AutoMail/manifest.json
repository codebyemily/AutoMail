{
  "name": "AutoMail",
  "version": "1.0",
  "description": "Your personal Gmail Assistant",
  "key": "",

  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "FriendBlock, friends face's in a block."
  }, 
  "oauth2": {
    "client_id": "1043288152858-hji195h389k1mmojbrne9h86of4685bi.apps.googleusercontent.com",
     "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://mail.google.com/",
      "openid",
      "email",
      "profile"
    ]
  },
   "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
   "permissions": [
    "identity",
    "identity.email",
    "scripting",
    "activeTab",
    "storage"
  ],


  "host_permissions": [
    "*://*.google.com/*"
  ]
 
}